command /dupe:
    trigger:
        if player's tool is any shulker box:
            set {_nbt::*} to compound list tag "tag;BlockEntityTag;Items" of full nbt compound of player's tool
            loop item from nbt {_nbt::*}:
                set {_item} to loop-value
                if {blacklist::*} contains type of {_item}:
                    message "&cThat greater than or equal to 1 items that are on the blacklist, and cannot be duped."
                    stop
        if {blacklist::*} contains type of player's tool:
            message "&cThat item is blacklisted and cannot be duped."
            stop
        if player is holding air:
            message "&cYou are not holding an item!"
            stop
        set {_l} to player's held item
        set {_l2} to amount of {_l} in player's inventory
        give player {_l2} of {_l}
        send action bar "&aYou have duplicated your item!" to player
        add 1 to {dupes::%player's uuid%}

command /blacklist [<string>]:
    trigger: 
        if arg-1 is not "add" or "remove" or "clear":
            send "&eBlacklist contains:"
            send ""
            send "&e%{blacklist::*}%"
            send ""
            stop
        if arg-1 is "add":
            if player has permission "blacklist.add":
                if type of player's tool is air:
                    message "&fThat is not a valid item!"
                    stop
                if {blacklist::*} contains type of player's tool:
                    message "&fThat item is already on the blacklist!"
                    stop
                add type of player's tool to {blacklist::*}
                message "&fYou successfully added &b%player's tool%&f to the blacklist."
                stop
            else:
                message "&cYou do not have permission to do that!"
                stop
        if arg-1 is "remove":
            if player has permission "blacklist.remove":
                if player's tool is air:
                    message "&fThat is not a valid item!"
                    stop
                if {blacklist::*} does not contain player's tool:
                    message "&fThat item is not on the blacklist!"
                    stop
                remove player's tool from {blacklist::*}
                message "&fYou successfully removed &b%player's tool%&f from the blacklist."
                stop
            else:
                message "&cYou do not have permission to do that!"
                stop
        if arg-1 is "clear":
            if player has permission "blacklist.clear":
                clear {blacklist::*}
                message "&fYou successfully cleared the blacklist."
                stop
            else:
                message "&cYou do not have permission to do that!"
                stop

