    set join message to "%player's display name% &7joined the server."
    wait 1 second
    if {kills::%player's uuid%} is not set:
        set {kills::%player's uuid%} to 0
    if {deaths::%player's uuid%} is not set:
        set {deaths::%player's uuid%} to 0
