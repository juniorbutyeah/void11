command /spawn [<text>]:
    trigger:
        set {_a} to arg-1 parsed as player
        if arg-1 is not set:
            teleport player to {spawn}
            send action bar "&aTeleported to spawn!"
        else if arg-1 is "set":
            if player has permission "set.spawn":
                set {spawn} to player's location
                send "&aSuccessfully set the spawn!"
            else:
                send "&cYou do not have permission to do that!"
        else if arg-1 is {_a}:
            if {_a} is online:
                teleport player to {_a}
                send "&aTeleported to &f%{_a}%&a!"
            else:
                send "&cThat player does not exist!"

on respawn:
